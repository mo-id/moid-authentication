{
  "name": "@mo-id/authentication",
  "repository": "git://github.com/mo-id/moid-authentication.git",
  "author": "moid",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "react",
    "nest",
    "types"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && sh ./scripts/postbuild.sh",
    "compile": "tsc",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "npm run build && husky install",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "cleanup": "sh ./scripts/cleanup.sh",
    "postpack": "npm run cleanup"
  },
  "peerDependencies": {
    "@nestjs/common": ">=8",
    "@nestjs/config": ">=1",
    "@nestjs/core": ">=8",
    "@nestjs/jwt": ">=8",
    "@nestjs/passport": ">=8",
    "bcrypt": ">=5",
    "axios": ">=0",
    "passport-jwt": ">=4",
    "react": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/moid-authentication.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/moid-authentication.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@mo-id/typescript-toolbelt": "^0.1.6",
    "@nestjs/common": "^8.4.3",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.3",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@testing-library/react": "^12.1.4",
    "@types/bcrypt": "^5.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "passport-jwt": "^4.0.0",
    "prettier": "^2.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  }
}
