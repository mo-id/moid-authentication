{
  "name": "@mo-id/authentication",
  "repository": "git://github.com/mo-id/moid-authentication.git",
  "author": "moid",
  "version": "0.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "compile": "tsc",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build && husky install",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "axios": ">=0",
    "react": ">=16"
  },
  "module": "dist/moid-authentication.esm.js",
  "size-limit": [
    {
      "path": "dist/moid-authentication.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/moid-authentication.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@mo-id/typescript-toolbelt": "^0.1.6",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@testing-library/react": "^12.1.4",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "axios": "^0.26.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md}": "prettier --write"
  }
}
